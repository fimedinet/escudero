{
    "name": "fimedinet/escudero",
    "description": "A set of tools for nutrition control used by dietitians",
    "license": "MIT",
    "authors": [
        {
            "name": "Ariel Vallese",
            "email": "ariel@fimedi.net",
            "homepage": "https://www.fimedi.net"
        }
    ],
    "homepage": "https://github.com/fimedinet/escudero",
    "keywords": ["Laravel", "escudero"],
    "require": {
        "illuminate/support": "~5",
        "ozdemirburak/json-csv": "^0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0 || ~7.0",
        "orchestra/testbench": "~3.5 || ~3.6",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "FimediNET\\Escudero\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FimediNET\\Escudero\\EscuderoServiceProvider"
            ],
            "aliases": {
                "escudero": "FimediNET\\Escudero\\Facades\\Escudero"
            }
        }
    }
}
